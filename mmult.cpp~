#include<iostream>
#include<vector>
#include<thread>
using namespace std;

vector<vector<int>> mat;

int main(int argc, char*argv[])
{
  for(int i =0 ; i< 4; i++)
    {
      for(int j = 0;j<4;j++)
	{
	  mat.push_back(i+i*j);
	}
    }

  for(auto x:mat)
    {
      for(auto j:x)
	{
	  cout << j <<" ";
	}
      cout << endl;
    }
  return 0;
}
